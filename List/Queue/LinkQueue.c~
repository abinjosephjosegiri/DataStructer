typedef int ElemType;
typedef struct Node{
	ElemType data;
	struct Node *Next;
}QNode; 
typedef struct{
	QNode  *front;//???§Ö??????????§Ö???????? 
	QNode  *rear; //???§Ö?¦Â?????????????????? 
}LinkQueue;

//???????????????????front->next??????????????????front->next != NULL,??????????????????????????
ElemType DeleteQ(LinkQueue *PtrQ){
	ElemType e;
	QNode *p;
	if(PtrQ->front == NULL){
		printf("???????");
		return NULL;
	}
	p = PtrQ->front;
	e = p->data;//????????? 
	if(PtrQ->front == PtrQ->rear){
		//???????????????¦Â?????????????NULL
		PtrQ->front = PtrQ->rear = NULL; 
	}else
		PtrQ->front = p->Next;
	free(p);
	return e;	
}
	

